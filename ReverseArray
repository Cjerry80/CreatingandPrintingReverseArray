#include <iostream>

using namespace std;

int main{

int array[100];
int size;

cout << "Enter the size of the array: " << endl;
cin >> size;

cout << "Enter the array elements" << endl;
for (int i = 0; i < size; i++){
cin >> array[i];
}

cout << "Now printing the array in reverse: ";
for (for (int i = 0; i < size; i++){
cout << array[i];
}

return 0;
}
